{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/admission/page.jsx", "app/api/auth/[...nextauth]/route.js", "app/api/auth/register/route.js", "app/colleges/[id]/page.jsx", "app/colleges/loading.jsx", "app/colleges/page.jsx", "app/login/page.jsx", "app/my-college/page.jsx", "app/profile/page.jsx", "app/register/page.jsx", "app/layout.jsx", "app/loading.jsx", "app/not-found.jsx", "app/page.jsx", "components/auth-provider.jsx", "components/footer.jsx", "components/navbar.jsx", "components/providers.jsx", "components/theme-provider.jsx", "hooks/use-auth.js", "hooks/use-mobile.jsx", "hooks/use-toast.js", "lib/utils.js", "tailwind.config.js"],
  "exclude": ["node_modules"]
}
